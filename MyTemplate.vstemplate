          </Folder>
          <ProjectItem ReplaceParameters="false" TargetFileName="Jenkinsfile">Jenkinsfile</ProjectItem>
        </Folder>
        <Folder Name="PROD" TargetFolderName="PROD">
          <Folder Name="openshift" TargetFolderName="openshift">
            <ProjectItem ReplaceParameters="false" TargetFileName="deployment.yml">deployment.yml</ProjectItem>
          </Folder>
          <ProjectItem ReplaceParameters="false" TargetFileName="Jenkinsfile">Jenkinsfile</ProjectItem>
        </Folder>
        <Folder Name="QA" TargetFolderName="QA">
          <Folder Name="openshift" TargetFolderName="openshift">
            <ProjectItem ReplaceParameters="false" TargetFileName="deployment.yml">deployment.yml</ProjectItem>
          </Folder>
          <ProjectItem ReplaceParameters="false" TargetFileName="Jenkinsfile">Jenkinsfile</ProjectItem>
        </Folder>
        <Folder Name="SUP" TargetFolderName="SUP">
