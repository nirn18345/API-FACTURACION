            <ProjectItem ReplaceParameters="false" TargetFileName="deployment.yml">deployment.yml</ProjectItem>
          </Folder>
          <ProjectItem ReplaceParameters="false" TargetFileName="Jenkinsfile">Jenkinsfile</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="Controllers" TargetFolderName="Controllers">
        <ProjectItem ReplaceParameters="true" TargetFileName="EjemploController.cs">EjemploController.cs</ProjectItem>
      </Folder>
      <Folder Name="Datos" TargetFolderName="Datos">
        <ProjectItem ReplaceParameters="true" TargetFileName="MapeoDatosEjemplo.cs">MapeoDatosEjemplo.cs</ProjectItem>
      </Folder>
      <Folder Name="Entidades" TargetFolderName="Entidades">
		<Folder Name="Consultas" TargetFolderName="Consultas">
			<ProjectItem ReplaceParameters="true" TargetFileName="ConsultarEjemploQuery.cs">ConsultarEjemploQuery.cs</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="Ejemplo.cs">Ejemplo.cs</ProjectItem>
