<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Plantilla Microservicios GrupoDifare</Name>
    <Description>Plantilla de proyecto para la creaci√≥n de API's .NET Core para aplicaciones de GRUPO DIFARE.</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Plantilla Microservicios GrupoDifare</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.jfif</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="MicroserviciosGD1.csproj" File="MicroserviciosGD1.csproj" ReplaceParameters="true">
      <Folder Name="Properties" TargetFolderName="Properties">
