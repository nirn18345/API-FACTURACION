		</Folder>
		<Folder Name="Operaciones" TargetFolderName="Operaciones">
			<ProjectItem ReplaceParameters="true" TargetFileName="GrabarEjemploRequest.cs">GrabarEjemploRequest.cs</ProjectItem>
